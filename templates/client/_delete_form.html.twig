<form method="post" action="{{ path('client_delete', {'clId': client.clId}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ client.clId) }}">
    <button class="btn">Delete</button>
</form>
